{
  "name": "react-tic-tac-toe",
  "version": "1.0.0",
  "description": "Tic-Tac-Toe game using React",
  "main": "src/index.jsx",
  "scripts": {
    "dev": "node -r esm ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack/webpack.config.dev.js",
    "build:dev": "node -r esm ./node_modules/webpack/bin/webpack.js --config webpack/webpack.config.dev.js",
    "build:prod": "node -r esm ./node_modules/webpack/bin/webpack.js --config webpack/webpack.config.prod.js",
    "serve:dev": "npm run build:dev && serve -s ./dist -l 8080",
    "serve:prod": "npm run build:prod && serve -s ./dist -l 8080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3a4ik/react-tic-tac-toe.git"
  },
  "author": "Denis Voloshin <denvol1996@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3a4ik/react-tic-tac-toe/issues"
  },
  "homepage": "https://github.com/3a4ik/react-tic-tac-toe#readme",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "dotenv": "^8.2.0",
    "immer": "^5.1.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.17.0",
    "esm": "^3.2.25",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^5.5.0",
    "serve": "^11.3.0",
    "terser-webpack-plugin": "^2.3.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  }
}
